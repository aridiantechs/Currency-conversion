{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-Waheed"
		},
		"LS_des_POC_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_des_POC'"
		},
		"dest_azure_colzani_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'dest_azure_colzani'"
		},
		"poc_colzanilive_onprem_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'poc_colzanilive_onprem'"
		},
		"JsonCurrencyRate_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://openexchangerates.org"
		},
		"JsonUSDTOPKR_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://aridianweb.atdemo.live"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-Waheed_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_des_POC_connectionString": {
						"value": "[parameters('LS_des_POC_connectionString')]"
					},
					"dest_azure_colzani_connectionString": {
						"value": "[parameters('dest_azure_colzani_connectionString')]"
					},
					"poc_colzanilive_onprem_connectionString": {
						"value": "[parameters('poc_colzanilive_onprem_connectionString')]"
					},
					"JsonCurrencyRate_properties_typeProperties_url": {
						"value": "[parameters('JsonCurrencyRate_properties_typeProperties_url')]"
					},
					"JsonUSDTOPKR_properties_typeProperties_url": {
						"value": "[parameters('JsonUSDTOPKR_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}